{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/api/feed/rss",
      "destination": "/feed.xml",
      "permanent": true
    },
    {
      "source": "/feed",
      "destination": "/playground/feed",
      "permanent": true
    },
    {
      "source": "/hawaii",
      "destination": "/logbook/big-island-hawaii",
      "permanent": true
    },
    {
      "source": "/api/og",
      "destination": "/api/og.mjs",
      "permanent": true
    },
    {
      "source": "/api/ghost-hook",
      "destination": "/api/ghost-hook.ts",
      "permanent": true
    }
  ]
}

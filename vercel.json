{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4",
      "includeFiles": "**/*"
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/dist/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(css|js|img|fonts)/(.*)",
      "dest": "/public/$1/$2",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/$1",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate=86400" }
    },
    {
      "src": "/api/feed/rss",
      "dest": "/api/feed.php",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate=86400" }
    },
    {
      "src": "/feed.xml",
      "dest": "/api/feed.php",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate=86400" }
    },
    {
      "src": "/feed",
      "status": 308,
      "headers": { "Location": "/playground/feed" }
    },
    {
      "src": "/hawaii",
      "status": 308,
      "headers": { "Location": "/logbook/big-island-hawaii" }
    },
    {
      "src": "/api/og",
      "dest": "/api/og.mjs"
    },
    {
      "src": "/api/ghost-hook",
      "dest": "/api/ghost-hook.ts"
    },
    {
      "src": "/",
      "dest": "/api/index.php",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate=86400" }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php",
      "headers": { "Cache-Control": "s-maxage=1, stale-while-revalidate=86400" }
    }
  ]
}
